<body>

    <div style="position: absolute; z-index: 999; right: 0; top: 10%; left: 60%; transform: translate(-50%,-50%);">
        <% if (messages.error) { %>
            <div class="alert alert-danger" role="alert" style="width: max-content;">
                <%= messages.error %>
            </div>
            <% } else if (messages.warning) { %>
                <div class="alert alert-warning" role="alert" style="width: max-content;">
                    <%= messages.warning %>
                </div>
                <% } else if (messages.success) { %>
                    <div class="alert alert-success" role="alert" style="width: max-content;">
                        <%= messages.success %>
                    </div>
                    <% } %>
    </div>
    <div class="min-h-100 p-0 p-sm-6 d-flex align-items-stretch">
        <div class="card w-25x flex-grow-1 flex-sm-grow-0 m-sm-auto">
            <div class="card-body p-sm-5 m-sm-3 flex-grow-0">
                <h1 class="mb-0 fs-3">Sign In</h1>
                <div class="fs-exact-14 text-muted mt-2 pt-1 mb-5 pb-2">Log in to your account to continue.</div>
                <form action="/admin/adminsignin" method="post">
                    <div class="mb-4"><label class="form-label">Email Address</label><input type="email"
                            class="form-control form-control-lg" name="email" required /></div>
                    <div class="mb-4"><label class="form-label">Password</label><input type="password"
                            class="form-control form-control-lg" name="password" required /></div>
                    <div class="mb-4 row py-2 flex-wrap">
                        <div class="col-auto me-auto"><label class="form-check mb-0"><input type="checkbox"
                                    class="form-check-input" /><span class="form-check-label">Remember me</span></label>
                        </div>
                        <div class="col-auto d-flex align-items-center"><a href="">Forgot password?</a></div>
                    </div>
                    <div><button class="btn btn-primary btn-lg w-100">Sign In</button></div>
                </form>
            </div>
            <div class="card-body p-sm-5 m-sm-3 flex-grow-0">
                <div class="form-group mb-0  pt-2 text-center text-muted">Don&#x27;t have an account? <a
                        href="/admin/singup">Sign up</a></div>
            </div>
        </div>
    </div><!-- scripts -->

</body>

</html>