<%- include('../partials/nav.ejs') %>
   <div id="top" class="sa-app__body">
      <div class="mx-sm-2 px-2 px-sm-3 px-xxl-4 pb-6">
         <div class="container container--max--xl">
            <div class="py-5">
               <div class="row g-4 align-items-center">
                  <div class="col">
                     <h1 class="h3 m-0">Add Shipping</h1>
                  </div>
               </div>
            </div>
            <div class="sa-entity-layout"
               data-sa-container-query="{&quot;920&quot;:&quot;sa-entity-layout--size--md&quot;,&quot;1100&quot;:&quot;sa-entity-layout--size--lg&quot;}">
               <div class="sa-entity-layout__body">
                  <div class="sa-entity-layout__main">
                     <div class="card">
                        <div class="card-body p-5">
                           <form action="/admin/shipping_add" method="post">
                              <div class="mb-4 d-flex gap-5">
                                 <label class="form-check">
                                    <input type="radio" class="form-check-input" name="status" value="Enabled">
                                    <span class="form-check-label">Enabled</span>
                                 </label>
                                 <label class="form-check">
                                    <input type="radio" class="form-check-input" name="status" value="Disabled">
                                    <span class="form-check-label">Disabled</span>
                                 </label>
                                 <label class="form-check mb-0">
                              </div>
                              <div class="mb-4"><label for="form-coupon/code" class="form-label">Price</label><input
                                    type="text" class="form-control" id="form-coupon/code" name="price"
                                    placeholder="Enter Price" />
                              </div>
                              <button class="btn btn-primary">Save</button>
                           </form>
                        </div>
                        <table class="sa-datatables-init text-nowrap">
                        <thead>
                           <tr>
                              <th>Shipping</th>
                              <th>Price</th>
                              <th>Status</th>
                           </tr>
                        </thead>
                        <tbody>
                           <% shippings.forEach((shipping, index) => { %>
                           <tr>
                              <td><a href="app-coupon.html" class="text-reset">Shipping</a></td>
                              <td><%= shipping.price %></td>
                              <td>
                                  <div class="d-flex fs-16">
                                      <div class="badge badge-sa-pill <% if (shipping.status === 'Disabled') { %> badge-sa-success <% } else { %> badge-sa-primary <% } %>"><%= shipping.status %></div>
                                  </div>
                              </td>
                          </tr>
                          <% }); %>
                        </tbody>
                     </table>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
   <%- include('../partials/footer.ejs') %>