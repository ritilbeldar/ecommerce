<%- include('partials/nav.ejs') %>
<main>
   <section class="swiper-container js-swiper-slider slideshow full-width_padding" data-settings='{
      "autoplay": {
      "delay": 5000
      },
      "slidesPerView": 1,
      "effect": "fade",
      "loop": true,
      "pagination": {
      "el": ".slideshow-pagination",
      "type": "bullets",
      "clickable": true
      }
      }'>
      <div class="swiper-wrapper">
         <% homebanners.forEach((banners, index)=> {
            const currentDate = new Date();
            
            if (banners.status === 'published' && !banners.isHidden) {
            %>
         <div class="swiper-slide full-width_border border-1" style="border-color: #f5e6e0;">
            <div class="overflow-hidden position-relative h-100">
               <div class="slideshow-bg" style="background-color: #f5e6e0;">
                  <img loading="lazy" src="<%= banners.backgroundBanner.url %>" width="1761" height="778"
                     alt="Pattern" class="slideshow-bg__img object-fit-cover">
               </div>
               <!-- <p class="slideshow_markup font-special text-uppercase position-absolute end-0 bottom-0">Summer</p> -->
               <div class="slideshow-character position-absolute bottom-0 pos_right-center">
                  <img loading="lazy" src="<%= banners.bannerImg.url %>" width="400" height="733"
                     alt="Woman Fashion 1"
                     class="slideshow-character__img animate animate_fade animate_btt animate_delay-9 h-auto w-auto">
                  <div class="character_markup">
                     <p
                        class="text-uppercase font-sofia fw-bold animate animate_fade animate_rtl animate_delay-10">
                        <%= banners.shortTitle1 %>
                     </p>
                  </div>
               </div>
               <div class="slideshow-text container position-absolute start-50 top-50 translate-middle">
                  <h6
                     class="text_dash text-uppercase text-red fs-base fw-medium animate animate_fade animate_btt animate_delay-3">
                     <%= banners.bigTitle2 %>
                  </h6>
                  <h2 class="text-uppercase h1 fw-normal mb-0 animate animate_fade animate_btt animate_delay-5">
                     <%= banners.bigTitle1 %>
                  </h2>
                  <h6 class="text-uppercase mb-5 animate animate_fade animate_btt animate_delay-3">
                     <%= banners.bigTitle2 %>
                  </h6>
                  <a href="<%= banners.bannerURL %>"
                     class="btn-link btn-link_lg default-underline text-uppercase fw-medium animate animate_fade animate_btt animate_delay-7">
                  <%= banners.bannerURLName %>
                  </a>
               </div>
            </div>
         </div>
         <% } else if (banners.status==='scheduled' && new Date(banners.publishDate) <=currentDate) { %>
         <div class="swiper-slide full-width_border border-1" style="border-color: #f5e6e0;">
            <div class="overflow-hidden position-relative h-100">
               <div class="slideshow-bg" style="background-color: #f5e6e0;">
