   <section class="collections-grid collections-grid_masonry" id="section-collections-grid_masonry">
      <div class="container h-md-100">
         <div class="row h-md-100">
            <% categorys.forEach((category, index)=> { %>
            <% category.subcategories.forEach((subcategory)=> { %>
            <% if (subcategory.bannerPosition==='True' ) { %>
            <div class="col-lg-6 d-flex flex h-50">
               <div class="collection-grid__item position-relative flex-grow-1 mb-lg-4">
                  <div class="background-img"
                     style="background-image: url('<%= subcategory.subCategoryBanner.url %>');"></div>
                  <div class="content_abs content_bottom content_left content_bottom-md content_left-md">
                     <p class="text-uppercase mb-1">
                        <%= subcategory.shortTitle %>
                     </p>
                     <h3 class="text-uppercase"><strong>
                        <%= subcategory.title %>
                        </strong>
                        <%= subcategory.title2 %>
                     </h3>
                     <% if (isAuthenticated) { %>
                     <a href="/user/subcategory_Products/<%= subcategory.id %>"
                        class="btn-link default-underline text-uppercase fw-medium">Shop Now</a>
                     <% } else { %>
                     <a href="/subcategory_Products/<%= subcategory.id %>"
                        class="btn-link default-underline text-uppercase fw-medium">Shop Now</a>
                     <% } %>
                  </div>
                  <!-- /.content_abs content_bottom content_left content_bottom-md content_left-md -->
               </div>
            </div>
            <% } %>
            <% }); %>
            <% }); %>
         </div>
         <!-- /.row -->
      </div>
      <!-- /.container -->
   </section>
