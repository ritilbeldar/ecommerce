            <span class="ms-2">RESET FILTER</span>
            </button>
         </div>
      </div>
   </div>
   <!-- /.aside-content -->
</div>
<!-- Customer Login Form -->
<% if (isAuthenticated) { %>
<div class="aside aside_right overflow-hidden cart-drawer" id="cartDrawer">
   <div class="aside-header d-flex align-items-center">
      <h3 class="text-uppercase fs-6 mb-0">SHOPPING BAG (<span class="cart-amount js-cart-items-count">
         <%= user.UserWishlists.length %>
         </span>)
      </h3>
      <button class="btn-close-lg js-close-aside btn-close-aside ms-auto"></button>
   </div>
   <!-- /.aside-header -->
   <% const wishlistItemss=user.UserWishlists.map(wishlist=> wishlist.allProduct); %>
   <div class="aside-content cart-drawer-items-list">
      <% let total=0; %>
      <% user.UserWishlists.forEach((wishlistproduct, index)=> { %>
      <% const correspondingProduct=wishlistItemss ? wishlistItemss[index] : null; %>
      <div class="cart-drawer-item d-flex position-relative">
         <div class="position-relative">
            <a href="/user/products_details/<%= correspondingProduct.id %>">
            <img loading="lazy" class="cart-drawer-item__img"
               src="<%= correspondingProduct ? correspondingProduct.productsImages[0].url : '' %>"
               alt="">
            </a>
         </div>
         <div class="cart-drawer-item__info flex-grow-1">
            <h6 class="cart-drawer-item__title fw-normal">
               <%= correspondingProduct ? correspondingProduct.name.split(' ').slice(0, 3).join(' ') : '' %>
               <% if (correspondingProduct && correspondingProduct.name.split(' ').length > 3) { %>
               ...
               <% } %>
            </h6>
            <% if (wishlistproduct.color) { %>
            <p class="cart-drawer-item__option text-secondary text-capitalize">Color: <%= wishlistproduct.color %></p>
            <% } else if (correspondingProduct && correspondingProduct.productsColor && correspondingProduct.productsColor.length > 0) { %>
            <p class="cart-drawer-item__option text-secondary text-capitalize">Color: <%= correspondingProduct.productsColor[0].colorname %></p>
            <% } %>
            <% if (wishlistproduct.size) { %>
            <p class="cart-drawer-item__option text-secondary">Size: <%= wishlistproduct.size %></p>
            <% } else if (correspondingProduct && correspondingProduct.sizes.length > 0) { %>
            <p class="cart-drawer-item__option text-secondary">Size: <%= correspondingProduct.sizes[0] %></p>
            <% } %>
            <div class="d-flex align-items-center justify-content-between mt-1">
               <div class="qty-control position-relative">
                  <input type="number" name="quantity" value="<%= wishlistproduct.quantity || ' 1' %>"
                     min="1" class="qty-control__number border-0 text-center">
                  <div class="qty-control__reduce text-start">-</div>
                  <div class="qty-control__increase text-end">+</div>
               </div>
               <!-- .qty-control -->
               <% let totalPrice=(correspondingProduct ? correspondingProduct.discountPrice : 0) *
                  (wishlistproduct.quantity || 1); %>
               <% total +=totalPrice; %>
               <span class="cart-drawer-item__price money price">₹<%= totalPrice %></span>
            </div>
         </div>
         <!-- /.cart-drawer-item__info -->
         <a href="/user/remove_ProductTo_Wishlist/<%= correspondingProduct ? correspondingProduct.id : '' %>"
            class="btn-close-xs position-absolute top-0 end-0"></a>
      </div>
      <hr class="cart-drawer-divider">
      <% }); %>
   </div>
   <!-- /.aside-content -->
   <div class="cart-drawer-actions position-absolute start-0 bottom-0 w-100">
      <hr class="cart-drawer-divider">
      <div class="d-flex justify-content-between">
         <% let totalDiscountPrice=0; wishlistItemss.forEach(product=> {
            totalDiscountPrice += product.discountPrice;
            });
            %>
         <h6 class="fs-base fw-medium">SUBTOTAL:</h6>
         <span class="cart-subtotal fw-medium">₹<%= total.toFixed(2) %></span>
      </div>
      <!-- /.d-flex justify-content-between -->
      <% if (user.UserWishlists.length !==0) { %>
      <a href="/user/shopCart" class="btn btn-light mt-3 d-block">View Cart</a>
      <a href="/user/shopCheckout" class="btn btn-primary mt-3 d-block">Checkout</a>
      <% } %>
   </div>
   <!-- /.aside-content -->
</div>
<% } %>

<div id="scrollTop" class="visually-hidden end-0"></div>
<div class="page-overlay"></div>
<!-- External JavaScripts -->
<script src="/frontend/js/plugins/jquery.min.js"></script>
<script src="/frontend/js/plugins/bootstrap.bundle.min.js"></script>
<script src="/frontend/js/plugins/bootstrap-slider.min.js"></script>
<script src="js/plugins/jquery.waypoints.min.js"></script>
<script src="js/plugins/sticky.min.js"></script>
<script src="/frontend/js/plugins/swiper.min.js"></script>
<script src="/frontend/js/plugins/countdown.js"></script>
<!-- Footer Scripts -->
<script src="/frontend/js/theme.js"></script>
<script>
   document.addEventListener("DOMContentLoaded", function () {
      var alertDiv = document.querySelector(".alert");
   
      if (alertDiv) {
         setTimeout(function () {
            alertDiv.style.display = "none";
         }, 3000);
      }
   });
</script>
</body>
</html>